{
	"name": "heimdall",
	"version": "1.5.0",
	"buildVersion": "",
	"scripts": {
		"ng": "ng",
		"serve": "ng serve",
		"serve:prod": "ng serve --configuration production",
		"serve:live": "ng serve --configuration live",
		"serve:devServer": "cd devServer && node ./devServer.js",
		"serve:localServer": "ng serve --configuration localServer",
		"build": "ng build",
		"build:prod": "ng build --configuration production",
		"build:prodExplore": "ng build --configuration production --source-map=true --named-chunks=true && source-map-explorer dist/heimdall/**/*.js",
		"build:explore": "source-map-explorer dist/heimdall/**/*.js",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"test:cI": "ng test --browsers ChromeHeadless --watch=false",
		"prepare": "cd .. && husky install heimdall/.husky",
		"lint": "ng lint",
		"lint:fix": "ng lint --fix",
		"prettier": "prettier . --write",
		"prettier:staged": "pretty-quick --staged",
		"showTestReport": "open-cli ./coverage/heimdall/lcov-report/index.html"
	},
	"private": true,
	"dependencies": {
		"@angular-devkit/build-angular": "^19.1.8",
		"@angular/animations": "~19.1.7",
		"@angular/common": "~19.1.7",
		"@angular/compiler": "~19.1.7",
		"@angular/core": "~19.1.7",
		"@angular/forms": "~19.1.7",
		"@angular/material": "^19.1.5",
		"@angular/platform-browser": "~19.1.7",
		"@angular/platform-browser-dynamic": "~19.1.7",
		"@angular/router": "~19.1.7",
		"@microsoft/signalr": "^7.0.12",
		"@ng-bootstrap/ng-bootstrap": "^18.0.0",
		"@ng-web-apis/common": "^3.0.6",
		"@popperjs/core": "^2.11.6",
		"@turf/turf": "^7.2.0",
		"angular-animations": "^0.11.0",
		"angular-oauth2-oidc": "^15.0.1",
		"backoff-rxjs": "^7.0.0",
		"bootstrap": "^5.2.3",
		"geolib": "^3.3.4",
		"maplibre-gl": "^3.6.2",
		"ng-animate": "^2.0.1",
		"ngx-timeago": "^3.0.0",
		"rxjs": "~7.5.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular-eslint/builder": "^19.1.0",
		"@angular-eslint/eslint-plugin": "19.1.0",
		"@angular-eslint/eslint-plugin-template": "19.1.0",
		"@angular-eslint/schematics": "^19.1.0",
		"@angular-eslint/template-parser": "19.1.0",
		"@angular/cli": "^19.1.8",
		"@angular/compiler-cli": "~19.1.7",
		"@angular/localize": "~19.1.7",
		"@types/geojson": "^7946.0.16",
		"@types/gtag.js": "^0.0.13",
		"@types/jasmine": "~3.10.0",
		"@types/node": "^12.11.1",
		"@types/signalr": "^2.4.2",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jasmine-core": "~4.0.0",
		"karma": "~6.3.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.1.0",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "~1.7.0",
		"lint-staged": "^13.2.2",
		"open-cli": "^7.2.0",
		"prettier": "2.8.8",
		"prettier-eslint": "^15.0.1",
		"pretty-quick": "^3.1.3",
		"source-map-explorer": "^2.5.3",
		"ts-node": "^10.9.1",
		"typescript": "~5.7.3"
	}
}
